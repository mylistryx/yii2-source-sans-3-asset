{
  "name": "mylistryx/yii2-source-sans-3-asset",
  "description": "Yii2 Source Sans 3 font asset bundle",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "authors": [
    {
      "name": "Sergey Zhukovskiy",
      "email": "mylistryx@gmail.com",
      "homepage": "https://net23.ru"
    }
  ],
  "keywords": [
    "yii2",
    "source sans 3",
    "asset",
    "bundle",
    "free",
    "font"
  ],
  "version": "1.0.0",
  "readme": "README.md",
  "autoload": {
    "psr-4": {
      "yii\\SourceSans3\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],  
  "require": {
    "php": ">=8.2",
    "yiisoft/yii2": "~2.0.50"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
